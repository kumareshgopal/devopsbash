         *  Dts.Connections["Sales DB"].ReleaseConnection(rawConnection);
         *
         * Example of using a File connection manager
         *  object rawConnection = Dts.Connections["Prices.zip"].AcquireConnection(Dts.Transaction);
         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            // TODO: Add your code here ConnectionManager 
            ConnectionManager cm;
            System.Data.SqlClient.SqlConnection sqlConn;
            System.Data.SqlClient.SqlCommand sqlComm;
            SqlDataReader SqlRead;
          //  string strHeader;
            cm = Dts.Connections["BiMetadata ADO NET"];

            //string strEnvironmentName = Dts.Variables["$Package::EnvironmentName"].Value.ToString();
            
            String strHTMLREport;

            
            String sql = null;

            String batch = Dts.Variables["$Package::BatchId"].Value.ToString();


            sql = "SELECT convert(nvarchar,[SourceTableName]) ,convert(nvarchar,[ReportingPeriodID]) ,convert(nvarchar,[RunType]) ,convert(nvarchar,[ActionType]) ,[ProcessStatus] ,convert(nvarchar,RowsProcessed) AS [RowsProcessed]" +
            " FROM [Bimetadata].[ArchiveControl].[WarehouseArchiveControl] WHERE ActionType = 'Archive' and BatchID = " + batch ;
           

            sqlConn = (System.Data.SqlClient.SqlConnection)cm.AcquireConnection(Dts.Transaction);
            sqlComm = new System.Data.SqlClient.SqlCommand(sql, sqlConn);
            SqlRead = sqlComm.ExecuteReader();


            // strHeader = " Daily Backup And Restore Job Status ";

            if (SqlRead.HasRows)
            {

                strHTMLREport = "<html><body><tr><td><b> Job Name: BI_WEEKEND_Archive </b></td></tr><br>";
                strHTMLREport = strHTMLREport + "<tr><td><b> Job Status: Completed </b></td> </tr><br> ";
                strHTMLREport = strHTMLREport + "<tr><td><b> Batch ID: " + batch + " </b></td> </tr><br> ";
                strHTMLREport = strHTMLREport + "<table border='1'>";

                //" + Dts.Variables["$Package::BatchID"].Value.ToString() + "

                strHTMLREport = strHTMLREport + "<tr><th>TableName</th><th>ReportingPeriodID</th><th>RunType</th><th>ActionType</th><th>ProcessStatus</th><th>RowsProcessed</th></tr>";
                

                while (SqlRead.Read())
                {
                    strHTMLREport = strHTMLREport + "<tr><td width='100' align='left'><b>" + SqlRead.GetValue(0).ToString() + "</b></td>";
                    strHTMLREport = strHTMLREport +     "<td width='50' align='left'><b>" + SqlRead.GetValue(1).ToString() + "</b></td>";
                    strHTMLREport = strHTMLREport +     "<td width='50' align='left'><b>" + SqlRead.GetValue(2).ToString() + "</b></td>";
                    strHTMLREport = strHTMLREport +     "<td width='50' align='left'><b>" + SqlRead.GetValue(3).ToString() + "</b></td>";
                    strHTMLREport = strHTMLREport +     "<td width='100' align='left'><b>" + SqlRead.GetValue(4).ToString() + "</b></td>";
                    strHTMLREport = strHTMLREport +     "<td width='50' align='left'><b>" + SqlRead.GetValue(5).ToString() + "</b></td></tr>";
                    

                }
                strHTMLREport = strHTMLREport + "</table></body></html>";
                //MessageBox.Show(strHTMLREport);

                //Dts.Variables["User::strHtmlReport"].Value = strHTMLREport;



                string SendMailSubject = Dts.Variables["System::MachineName"].Value.ToString() + "|" + "Talbot_Warehouse DB Data Archiving process has completed";
                // Get SMTP Server from SMTP Connection Manager. Alternative is to use extra variables or paramters instead:
                // String SmtpServer = Dts.Variables["SmtpServer"].Value.ToString();

                String SmtpServer = Dts.Connections["SMTP Connection Manager"].Properties["SmtpServer"].GetValue(Dts.Connections["SMTP Connection Manager"]).ToString();

                // Create an email and change the format to HTML
                MailMessage myHtmlFormattedMail = new MailMessage("noreply@talbotuw.com", Dts.Variables["User::EmailToBI"].Value.ToString(), SendMailSubject, strHTMLREport);
                myHtmlFormattedMail.IsBodyHtml = true;

                // Create a SMTP client to send the email
                SmtpClient mySmtpClient = new SmtpClient(SmtpServer);
                //mySmtpClient.Port = 2525; // If you want to use a different portnumber instead of the default. Else remove this line.
                mySmtpClient.Send(myHtmlFormattedMail);

            }



            cm.ReleaseConnection(sqlConn);

           

			Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
